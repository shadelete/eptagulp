{
  "name": "frontend_gulp_boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "gulp": "^3.9.1"
  },
  "config": {
    "dev": {
      "sass": "dev/sass/main.sass",
      "html": "dev/*.html",
      "js": "dev/js/main.js",
      "fonts": "dev/fonts/**/*.*",
      "img": {
        "compress": [
          "dev/img/**/*.*",
          "!dev/img/noCompress/**/*.*"
        ],
        "noCompress": "dev/img/noCompress/**/*.*"
      }
    },
    "dist": {
      "css": "dist/css",
      "html": "dist/",
      "js": "dist/js/",
      "fonts": "dist/fonts",
      "img": "dist/img"
    },
    "libs": {
      "js": "dev/js/libs.js",
      "css": "dev/css/libs.css"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.1.0",
    "browser-sync": "^2.23.7",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-bower": "0.0.14",
    "gulp-cli": "^2.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-file-include": "^2.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-import-css": "^0.1.3",
    "gulp-include": "^2.3.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "require-dir": "^1.0.0",
    "rfs": "^7.1.1",
    "run-sequence": "^2.2.1"
  },
  "scripts": {
    "start": "gulp"
  },
  "author": "shadelete",
  "license": "ISC"
}
